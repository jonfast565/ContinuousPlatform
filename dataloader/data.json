{
  "Applications": [
    {
      "Repository": "WebNCDR",
      "Solution": "WebNCDR.sln",
      "Project": "NCDRSitefinity.csproj",
      "Resources": {
        "IisApplications": [
          "WebNCDR"
        ]
      },
      "Type": "Web"
    }
  ],
  "IisSites": [
    {
      "Name": "www.ncdr.com",
      "PhysicalPath": "C:\\Websites\\www.ncdr.com",
      "ApplicationPool": {
        "Name": "www.ncdr.com",
        "ProcessType": "Integrated",
        "FrameworkVersion": "v4.0"
      },
      "IisApplications": [
        "WebNCDR"
      ],
      "Environments": [
        {
          "BusinessLine": "Ncdr",
          "Name": "Production"
        }
      ]
    }
  ],
  "IisApplications": [
    {
      "Name": "WebNCDR",
      "VirtualPath": "/WebNCDR",
      "PhysicalPath": "C:\\Websites\\{{SiteName}}\\WebNCDR",
      "ApplicationPool": {
        "Name": "WebNCDR-{{SiteName}}",
        "ProcessType": "Integrated",
        "FrameworkVersion": "v4.0"
      }
    }
  ],
  "WindowsServices": [
    {
      "Name": "NCDR Export Processor",
      "BinaryPath": "C:\\WindowsApp\\{{EnvironmentName}}\\Ncdr.UnifiedPlatform.Console.ExportProcessor",
      "BinaryExecutableName": "Ncdr.UnifiedPlatform.Console.ExportProcessor.exe",
      "BinaryExecutableArguments": "--service",
      "LoadBalanced": true,
      "Environments": [
        {
          "BusinessLine": "Ncdr",
          "Name": "Production"
        }
      ]
    }
  ],
  "ScheduledTasks": [
    {
      "Names": [
        "DQR Service1",
        "DQR Service2",
        "DQR Service3",
        "DQR Service4",
        "DQR Service5"
      ],
      "BinaryPath": "C:\\WindowsApp\\DQR",
      "BinaryExecutableName": "SubmissionProcessor.exe",
      "BinaryExecutableArguments": "",
      "ScheduleType": "Daily",
      "RepeatInterval": 10000,
      "RepetitionDuration": 10000,
      "ExecutionTimeLimit": 10000,
      "Priority": 1,
      "LoadBalanced": true,
      "Environments": [
        {
          "BusinessLine": "Ncdr",
          "Name": "Production"
        }
      ]
    }
  ],
  "Environments": [
    {
      "BusinessLine": "Ncdr",
      "Name": "Production",
      "Servers": [
        {
          "Name": "prdncdrweb1",
          "Type": "Web"
        },
        {
          "Name": "prdncdrweb2",
          "Type": "Web"
        },
        {
          "Name": "prdncdrweb3",
          "Type": "Web"
        },
        {
          "Name": "prdncdrapp1",
          "Type": "App"
        },
        {
          "Name": "prdncdrapp2",
          "Type": "App"
        }
      ]
    }
  ]
}